<link rel="stylesheet" href="/css/login.css">
    <h1>Login</h1>
    <form id="loginForm">
        <label for="email">Email:</label>
        <input type="email" id="email" name="email" required>
        <br>
        <label for="password">Password:</label>
        <input type="password" id="password" name="password" required>
        <br>
        <button id="submitLogIn" type="submit">Login</button>
    </form>

        <h1>Sign Up</h1>
    <form id="signupForm">
        <label for="firstName">First Name:</label>
        <input type="firstName" id="firstName" name="firstName" required>
        <br>
        <label for="lastName">Last Name:</label>
        <input type="lastName" id="lastName" name="lastName" required>
        <br>
        <label for="emailSignUp">Email:</label>
        <input type="email" id="emailSignUp" name="emailSignUp" required>
        <br>
        <label for="passwordSignUp">Password:</label>
        <input type="password" id="passwordSignUp" name="passwordSignUp" required>
        <br>
        <button id="submitSignUp" type="submit">Login</button>
    </form>

<script src="/js/login.js"></script>